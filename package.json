{
  "name": "sword-app",
  "private": true,
  "version": "1.0.0",
  "description": "Sword app repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' nest build",
    "format": "prettier --write \"src/**/*.ts\" ",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "cd src/common/database/migrations && npx typeorm-ts-node-commonjs migration:generate -d ./../../../../ormconfig.ts",
    "migration:create": "cd src/common/database/migrations && npx typeorm-ts-node-commonjs migration:create",
    "migration:up": "npx typeorm-ts-node-esm migration:run -d ./ormconfig.ts",
    "migration:down": "npx typeorm-ts-node-esm migration:revert -d ./ormconfig.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/platform-socket.io": "^9.0.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@terra-money/terra.js": "^3.1.4",
    "@types/express-jwt": "^6.0.4",
    "@types/geojson": "^7946.0.10",
    "@types/nodemailer": "^6.4.3",
    "@types/qrcode": "^1.4.2",
    "@types/speakeasy": "^2.0.7",
    "atob": "^2.1.2",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.21.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "joi": "^17.6.0",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "nest-router": "^1.0.9",
    "node-blob": "0.0.2",
    "node-fetch": "^2.6.7",
    "node-iplocate": "^1.0.2",
    "nodemailer": "^6.6.2",
    "npm": "^8.15.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfkit-table": "^0.1.99",
    "pg": "^8.7.3",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "serverless-tscpaths": "^0.0.7",
    "sniffr": "^1.2.0",
    "socket.io": "^4.5.4",
    "swagger-ui-dist": "^4.13.0",
    "swagger-ui-express": "^4.3.0",
    "tin-validator": "^1.0.0",
    "twilio": "^3.84.1",
    "typeorm": "0.3.9",
    "typeorm-transactional": "^0.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.4",
    "@types/auth0": "^2.35.3",
    "@types/aws-lambda": "^8.10.100",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.6",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.6.1",
    "@types/passport-local": "^1.0.33",
    "@types/request-ip": "0.0.37",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "plugin": "^0.3.3",
    "prettier": "^2.2.1",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleNameMapper": {
      "@common/(.*)": "<rootDir>/common/$1",
      "@resources/(.*)": "<rootDir>/resources/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "setupFiles": [
      "<rootDir>/../jest/.env.ts"
    ],
    "testEnvironment": "node"
  }
}
